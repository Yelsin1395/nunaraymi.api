{
  "name": "nunaraymi.api",
  "version": "1.0.0",
  "description": "Architecture monolitic of api",
  "private": true,
  "main": "dist/src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc && tsc-alias",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "npm run format -- --check",
    "format:fix": "npm run format -- --write",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepare": "husky install",
    "compile:app": "npm run lint && npm run test"
  },
  "author": "JESUS YELSIN BROLY GARCIA RIVERA",
  "license": "UNLICENSED",
  "dependencies": {
    "@logtail/node": "^0.4.6",
    "@logtail/winston": "^0.4.6",
    "bcryptjs": "^2.4.3",
    "cli-color": "^2.0.3",
    "compression": "^1.7.4",
    "connection-string": "^4.4.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "figlet": "^1.6.0",
    "helmet": "^7.0.0",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "memory-cache": "^0.2.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "rxjs": "^7.8.1",
    "rxjs-http-client": "^7.0.7",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.3",
    "@types/cli-color": "^2.0.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/figlet": "^1.5.6",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/memory-cache": "^0.2.3",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
